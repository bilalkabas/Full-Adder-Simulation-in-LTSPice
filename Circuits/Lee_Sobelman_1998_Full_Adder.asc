TEXT -392 16 Left 0 ;;Time values in ns when the pulse is halved:\n.meas TRAN tVXHL_temp WHEN V(pulse)=2.5 TD=5u\n.meas TRAN tVXLH_temp WHEN V(pulse)=2.5 TD=9.99u\n.meas TRAN tVXHL PARAM tVXHL_temp*1000000000\n.meas TRAN tVXLH PARAM tVXLH_temp*1000000000\n \n;Time values in ns when the Sum is halved:\n.meas TRAN VSUM_high FIND V(sum) AT=5u\n.meas TRAN VSUM_low FIND V(sum) AT=9.9999u\n.meas TRAN VSUM_half PARAM (VSUM_high+VSUM_low)/2\n.meas TRAN tVSUM1_temp WHEN V(sum)=VSUM_half TD=5u\n.meas TRAN  tVSUM1 PARAM tVSUM1_temp*1000000000\n.meas TRAN tVSUM2_temp WHEN V(sum)=VSUM_half TD=9.99u\n.meas TRAN  tVSUM2 PARAM tVSUM2_temp*1000000000\n \n;Time values in ns when the Cout is halved:\n.meas TRAN VCOUT_high FIND V(cout) AT=5u\n.meas TRAN VCOUT_low FIND V(cout) AT=9.9999u\n.meas TRAN VCOUT_half PARAM (VCOUT_high+VCOUT_low)/2\n.meas TRAN tVCOUT1_temp WHEN V(cout)=VCOUT_half TD=5u\n.meas TRAN  tVCOUT1 PARAM tVCOUT1_temp*1000000000\n.meas TRAN tVCOUT2_temp WHEN V(cout)=VCOUT_half TD=9.99u\n.meas TRAN  tVCOUT2 PARAM tVCOUT2_temp*1000000000\n \n;Propagation delays:\n.meas TRAN tP1_SUM PARAM tVSUM1-tVXHL\n.meas TRAN tP2_SUM PARAM tVSUM2-tVXLH\n.meas TRAN tP1_COUT PARAM tVCOUT1-tVXHL\n.meas TRAN tP2_COUT PARAM tVCOUT2-tVXLH
TEXT -400 392 Left 0 ;.meas TRAN min_vdd_xhl MIN V(vdd) FROM 5.00001u TO 10u\n.meas TRAN min_vdd_xlh MAX V(vdd) FROM 10u TO 15.00001u\n.meas TRAN P_diss_xhl_1 PARAM (5-min_vdd_xhl)*500\n.meas TRAN P_diss_xlh_1 PARAM (5-min_vdd_xlh)*500\n \n.meas TRAN avg_vdd_xhl AVG V(vdd) FROM 5.00001u TO 10u\n.meas TRAN avgDrop_xhl PARAM 5-avg_vdd_xhl\n.meas TRAN avg_vdd_xlh AVG V(vdd) FROM 10u TO 15.00001u\n.meas TRAN avgDrop_xlh PARAM 5-avg_vdd_xlh\n \n.meas TRAN P_diss_xhl PARAM avgDrop_xhl*500\n.meas TRAN P_diss_xlh PARAM avgDrop_xlh*500
TEXT -400 648 Left 0 !.meas TRAN avg_P_xhl AVG ((-V(VCC)*I(VCC)-(V(VDD,VCC)*I(R4))))*1000 FROM 5.00001u TO 10u\n.meas TRAN avg_P_xlh AVG ((-V(VCC)*I(VCC)-(V(VDD,VCC)*I(R4)))-(-V(Sum)*I(R2)+(-V(XNOR)*I(R1))+(-V(Cout)*I(R3))))*1000 FROM 10u TO 15.00001u\n \n.meas TRAN settled_P_xhl FIND ((-V(VCC)*I(VCC)-(V(VDD,VCC)*I(R4)))-(-V(Sum)*I(R2)+(-V(XNOR)*I(R1))+(-V(Cout)*I(R3)))-(V(N005,XNOR)*I(D1))-(V(N009,XNOR)*I(D2))-(V(N001,Sum)*I(D3))-(V(N002,Sum)*I(D4))-(V(N007,Cout)*I(D5))-(V(N008,Cout)*I(D6)))*1000 AT=9.99996u\n.meas TRAN settled_P_xlh FIND ((-V(VCC)*I(VCC)-(V(VDD,VCC)*I(R4)))-(-V(Sum)*I(R2)+(-V(XNOR)*I(R1))+(-V(Cout)*I(R3)))-(V(N005,XNOR)*I(D1))-(V(N009,XNOR)*I(D2))-(V(N001,Sum)*I(D3))-(V(N002,Sum)*I(D4))-(V(N007,Cout)*I(D5))-(V(N008,Cout)*I(D6)))*1000 AT=4.99996u
